{
  "env": {
    "browser": true,
    "es2021": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    // "quotes": [
    //   "error",
    //   "single"
    // ],
    "react/prop-types": "off", // Desactivar la regla react/prop-types
    "no-unused-vars": "off" // Desactivar la regla no-unused-vars
  }
}
